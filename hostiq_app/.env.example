# Azure Blob Storage Configuration
# Copy this file to .env and fill in your values

AZURE_STORAGE_ACCOUNT=your_storage_account
AZURE_CONTAINER_PATH=submissions/your_path

# SAS Token for submissions container (used by app.py and data_preparation.ipynb)
AZURE_SAS_TOKEN=your_submissions_sas_token

# SAS Token for airbnb container (used by Databricks notebooks)
AZURE_SAS_TOKEN_AIRBNB=your_airbnb_sas_token

# SAS Token for booking container (used by Databricks notebooks)
AZURE_SAS_TOKEN_BOOKING=your_booking_sas_token

BRIGHTDATA_USERNAME=your_brightdata_username
BRIGHTDATA_PASSWORD=your_brightdata_password
BRIGHTDATA_HOST=your_brightdata_host

# Walk Score API Key (get yours at https://www.walkscore.com/professional/api.php)
WALK_SCORE_API_KEY=your_walk_score_api_key